			</uniform>
			<uniform>
				<name>NormalTex</name>
				<type>sampler-2d</type>
				<value type="int">2</value>
			</uniform>
			<uniform>
				<name>depth_factor</name>
				<type>float</type>
				<value>
					<use>depth-factor</use>
				</value>
			</uniform>
			<uniform>
				<name>snowlevel</name>
