					<index>6</index>
				</attribute>
				<attribute>
					<name>binormal</name>
					<index>7</index>
				</attribute>
			</program>
			<uniform>
				<name>NoiseTex</name>
				<type>sampler-3d</type>
				<value type="int">0</value>
			</uniform>
			<uniform>
