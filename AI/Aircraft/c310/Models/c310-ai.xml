<?xml version="1.0"?>

<PropertyList>

 <path>c310-dpm.ac</path>
 
  <offsets>
    <z-m>.87</z-m>   
  </offsets>
  
 <animation>
  <type>select</type>
  <object-name>LeftPropeller</object-name>
  <condition>
   <less-than>
    <property>velocities/true-airspeed-kt</property>
    <value>41</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LeftPropeller.Fast</object-name>
  <condition>
   <greater-than>
    <property>velocities/true-airspeed-kt</property>
     <value>40</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>RightPropeller</object-name>
  <condition>
   <less-than>
    <property>velocities/true-airspeed-kt</property>
    <value>41</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>RightPropeller.Fast</object-name>
  <condition>
   <greater-than>
    <property>velocities/true-airspeed-kt</property>
    <value>40</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>LeftPropeller</object-name>
  <property>velocities/true-airspeed-kt</property>
    <factor> 90 </factor>
  <center>
   <x-m>-1.5</x-m>
   <y-m>-1.67</y-m>
   <z-m>0.3</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>RightPropeller</object-name>
  <property>velocities/true-airspeed-kt</property>
    <factor> 90 </factor>
  <center>
   <x-m>-1.5</x-m>
   <y-m>1.67</y-m>
   <z-m>0.3</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 <!-- 70kt takeoff, climb 80kt, cruise 100kt, desent 80kt, approch 70kt, touchdown 60kt -->
 <animation>
  <type>rotate</type>
  <object-name>NoseWheel</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
   <entry><ind>0.0</ind><dep>0.0</dep></entry>
   <entry><ind>70</ind><dep>0.0</dep></entry>
   <entry><ind>75</ind><dep>-90.0</dep></entry>
   <entry><ind>76</ind><dep>-90.0</dep></entry>
  </interpolation>
  <factor>120</factor>
  <offset-deg>-120</offset-deg>
  <min-deg>-90</min-deg>
  <max-deg>0</max-deg>
  <center>
   <x-m>-2.28</x-m>
   <y-m>0.0</y-m>
   <z-m>0.095</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LeftNoseWheelDoor</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
	<entry><ind>75</ind><dep>0.0</dep></entry>
	<entry><ind>76</ind><dep>90.0</dep></entry>
	<entry><ind>77</ind><dep>90.0</dep></entry>
  </interpolation>
  <factor>-360</factor>
  <offset-deg>90</offset-deg>
  <min-deg>0</min-deg>
  <max-deg>90</max-deg>
  <center>
   <x-m>-1.58</x-m>
   <y-m>-0.17</y-m>
   <z-m>-0.095</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>-0.05</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RightNoseWheelDoor</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
    <entry><ind>75</ind><dep>0.0</dep></entry>
    <entry><ind>76</ind><dep>-90.0</dep></entry>
    <entry><ind>77</ind><dep>-90.0</dep></entry>
  </interpolation>
  <factor>360</factor>
  <offset-deg>-90</offset-deg>
  <min-deg>-90</min-deg>
  <max-deg>0</max-deg>
  <center>
   <x-m>-1.58</x-m>
   <y-m>0.17</y-m>
   <z-m>-0.095</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>-.05</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LeftWheel</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
    <entry><ind>70</ind><dep>0.0</dep></entry>
    <entry><ind>75</ind><dep>80.0</dep></entry>
    <entry><ind>76</ind><dep>80.0</dep></entry>
  </interpolation>
  <factor>-80</factor>
  <offset-deg>80</offset-deg>
  <center>
   <x-m>0.2</x-m>
   <y-m>-1.8</y-m>
   <z-m>0.185</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RightWheel</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
    <entry><ind>70</ind><dep>0.0</dep></entry>
    <entry><ind>75</ind><dep>-80.0</dep></entry>
    <entry><ind>76</ind><dep>-80.0</dep></entry>
  </interpolation>
  <factor>80</factor>
  <offset-deg>-80</offset-deg>
  <center>
   <x-m>0.2</x-m>
   <y-m>1.8</y-m>
   <z-m>0.185</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>Step</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>0.0</dep></entry>
    <entry><ind>70</ind><dep>0.0</dep></entry>
    <entry><ind>75</ind><dep>-150.0</dep></entry>
    <entry><ind>76</ind><dep>-150.0</dep></entry>
  </interpolation>
  <factor>150</factor>
  <offset-deg>-150</offset-deg>
  <min-deg>-150</min-deg>
  <max-deg>0</max-deg>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.4</y-m>
   <z-m>0.05</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 
