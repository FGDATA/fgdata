        <alias> <!-- match any 777 type -->
            <match>777-</match>
            <alias>777</alias>
        </alias>
        <alias> <!-- match any 737 type -->
            <match>73</match>
            <alias>737</alias>
        </alias>
        
        <alias> <!-- match any 76x type -->
            <match>76</match>
            <match>75</match> <!-- and any 75x for the moment -->
            <alias>767</alias>
        </alias>
        
        <alias> <!-- match 747-400s -->
            <match>744</match>
            <match>747-4</match>
            <alias>747-400</alias>
        </alias>
        <alias> <!-- match MD11s -->
            <match>MD11</match> <!-- note this matches all MD11 prefixes - eg MD11/P -->
            <alias>MD11</alias>
        </alias>
        
        <alias>
            <match>346</match>
            <match>343</match>
            <alias>A340</alias>
        </alias>
        
        <alias> <!-- map the A330 to the 767 for the moment -->
            <match>A330</match>
            <alias>767</alias>
        </alias>
        
        <alias>
            <match>320</match>
            <match>319</match>
            <match>318</match>
            <match>321</match>
            <alias>A320</alias>
        </alias>
        
        <alias>
