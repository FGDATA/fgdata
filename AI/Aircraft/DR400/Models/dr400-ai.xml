  <model>
    <name>StrobeOff1</name>
    <path>light/ToggleLight.xml</path>
    <offsets>
      <x-m> -1.297 </x-m>
      <y-m> -4.253 </y-m>
      <z-m>  0.005 </z-m>
    </offsets>
  </model>
  
  <model>
    <name>StrobeOff2</name>
    <path>light/ToggleLight.xml</path>
    <offsets>
      <x-m> -1.297 </x-m>
