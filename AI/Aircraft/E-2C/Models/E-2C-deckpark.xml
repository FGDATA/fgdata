	<!-- ########## Wing Fold ########### -->
	<animation>
		<type>rotate</type>
		<object-name>Wing-R</object-name>
		<property>surface-positions/wing-fold-pos-norm</property>
    <offset>1.0</offset>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>117.5</dep></entry>
		</interpolation>
		<axis>
			<x1-m>-0.2567</x1-m>
			<y1-m>3.9999</y1-m>
			<z1-m>0.4230</z1-m>
			<x2-m>-0.4891</x2-m>
