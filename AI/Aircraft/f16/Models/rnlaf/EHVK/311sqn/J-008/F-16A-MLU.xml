  <object-name>CanopyGlassFwd</object-name>
 <property>fdm/jsbsim/fcs/canopy-pos-norm</property>
  <factor>30.0</factor>
  <center>
   <x-m>-3.30</x-m>
   <y-m>0</y-m>
   <z-m>0.6</z-m>
  </center>
