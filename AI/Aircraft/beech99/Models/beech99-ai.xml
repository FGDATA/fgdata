<!--gear-->
 <animation>
  <type>rotate</type>
  <object-name>Nose_Wheel</object-name>
  <property>velocities/true-airspeed-kt</property>
  <interpolation>
	<entry><ind>0.0</ind><dep>0.0</dep></entry>
	<entry><ind>60</ind><dep>0.0</dep></entry>
	<entry><ind>75</ind><dep>-120.0</dep></entry>
	<entry><ind>100</ind><dep>-120.0</dep></entry>
  </interpolation>
  <factor>120</factor>
  <offset-deg>-120</offset-deg>
  <min-deg>-90</min-deg>
  <max-deg>0</max-deg>
  <center>
   <x-m>-4.79</x-m>
   <y-m>0.0</y-m>
   <z-m>0.11</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
