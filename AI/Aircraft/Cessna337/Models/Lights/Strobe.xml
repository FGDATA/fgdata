<?xml version="1.0"?>

<PropertyList>
	
	  <path>Strobe.ac</path>
	  
	<animation>
		<type>alpha-test</type>
		<object-name>Strobe</object-name>
		<alpha-factor>0.001</alpha-factor>
	</animation>
	
	<animation>
		<type>billboard</type>
		<object-name>Strobe</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>Strobe</object-name>
		<interpolation>
	      <entry><ind>    0 </ind><dep> 0.25 </dep></entry>
	      <entry><ind>   20 </ind><dep> 0.50 </dep></entry>
	      <entry><ind> 1500 </ind><dep> 5.00 </dep></entry>
		</interpolation>
	</animation>

</PropertyList>