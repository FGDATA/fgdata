<?xml version="1.0"?>

<PropertyList>

  <path>c182rg.ac</path>

  <!-- LOD for the model as a whole -->
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10000</max-m>
  </animation>

  <!-- Object movements -->
  <animation>
    <type>select</type>
    <object-name>Propeller</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Propeller.2</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>400</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>blend</type>
    <object-name>Propeller.2</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>0.0004</factor>
    <max>1.0</max>
    <min>0.0</min>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Propeller</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-1</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.214544</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Propeller.2</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-1</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.214544</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftAileron</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>-20</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>1.22</x1-m>
      <y1-m>-2.546</y1-m>
      <z1-m>0.334</z1-m>
      <x2-m>0.761</x2-m>
      <y2-m>-5.101</y2-m>
      <z2-m>0.419</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightAileron</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>-20</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>1.22</x1-m>
      <y1-m>2.546</y1-m>
      <z1-m>0.334</z1-m>
      <x2-m>0.761</x2-m>
      <y2-m>5.101</y2-m>
      <z2-m>0.419</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftFlap</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>30</factor>
    <axis>
      <x1-m>1.219</x1-m>
      <y1-m>-2.542</y1-m>
      <z1-m>0.332</z1-m>
      <x2-m>1.131</x2-m>
      <y2-m>-0.599</y2-m>
      <z2-m>0.315</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightFlap</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>30</factor>
    <axis>
      <x1-m>1.131</x1-m>
      <y1-m>0.599</y1-m>
      <z1-m>0.315</z1-m>
      <x2-m>1.219</x2-m>
      <y2-m>2.542</y2-m>
      <z2-m>0.332</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LeftElevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>-28</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>23</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>5.138</x1-m>
      <y1-m>-1.657</y1-m>
      <z1-m>-0.475</z1-m>
      <x2-m>5.133</x2-m>
      <y2-m>-0.081</y2-m>
      <z2-m>-0.475</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RightElevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry>
        <ind>-1.0</ind>
        <dep>-28</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>23</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>5.133</x1-m>
      <y1-m>0.081</y1-m>
      <z1-m>-0.475</z1-m>
      <x2-m>5.138</x2-m>
      <y2-m>1.657</y2-m>
      <z2-m>-0.475</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>17.5</factor>
    <axis>
      <x1-m>4.976</x1-m>
      <y1-m>0.0</y1-m>
      <z1-m>-0.41</z1-m>
      <x2-m>5.938</x2-m>
      <y2-m>0</y2-m>
      <z2-m>0.942</z2-m>
    </axis>
  </animation>

  <!-- Gear Animation - nosewheel steering-->
  <animation>
    <type>rotate</type>
    <object-name>NoseWheel</object-name>
    <object-name>NoseWheelStrut</object-name>
    <object-name>NoseWheelFork</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>20</factor>
    <axis>
      <x1-m>-0.633</x1-m>
      <y1-m>0.0</y1-m>
      <z1-m>-0.706</z1-m>
      <x2-m>-0.773</x2-m>
      <y2-m>0.0</y2-m>
      <z2-m>-1.181</z2-m>
    </axis>
  </animation>

  <!-- Gear Animation - nosewheel -->
  <animation>
    <type>rotate</type>
    <object-name>NoseWheel</object-name>
    <object-name>NoseWheelStrut</object-name>
    <object-name>NoseWheelFork</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>95</dep>
      </entry>
      <entry>
        <ind>0.3</ind>
        <dep>95</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.639</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.722</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>NoseWheelDoorLeft</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.3</ind>
        <dep>-85</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>-85</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>-1.414</x1-m>
      <y1-m>-0.168</y1-m>
      <z1-m>-0.617</z1-m>
      <x2-m>-0.625</x2-m>
      <y2-m>-0.168</y2-m>
      <z2-m>-0.872</z2-m>
    </axis>
  </animation>

  <animation>
      <type>rotate</type>
      <object-name>NoseWheelDoorRight</object-name>
      <property>gear/gear[0]/position-norm</property>
      <interpolation>
        <entry>
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>0.3</ind>
          <dep>85</dep>
        </entry>
        <entry>
          <ind>1.0</ind>
          <dep>85</dep>
        </entry>
      </interpolation>
      <axis>
        <x1-m>-1.414</x1-m>
        <y1-m>0.168</y1-m>
        <z1-m>-0.617</z1-m>
        <x2-m>-0.625</x2-m>
        <y2-m>0.168</y2-m>
        <z2-m>-0.872</z2-m>
      </axis>
    </animation>

    <animation>
      <type>rotate</type>
      <object-name>LeftGear</object-name>
      <object-name>LeftWheel</object-name>
      <property>gear/gear[1]/position-norm</property>
      <interpolation>
        <entry>
          <ind>0</ind>
          <dep>180</dep>
        </entry>
        <entry>
          <ind>0.8</ind>
          <dep>0.0</dep>
        </entry>
        <entry>
          <ind>1.0</ind>
          <dep>0.0</dep>
        </entry>
      </interpolation>
      <axis>
        <x1-m>1.095</x1-m>
        <y1-m>-0.278</y1-m>
        <z1-m>-0.943</z1-m>
        <x2-m>1.31</x2-m>
        <y2-m>-0.55</y2-m>
        <z2-m>-1.0</z2-m>
      </axis>
    </animation>

    <animation>
      <type>rotate</type>
      <object-name>RightGear</object-name>
      <object-name>RightWheel</object-name>
      <property>gear/gear[1]/position-norm</property>
        <interpolation>
        <entry>
          <ind>0</ind>
          <dep>-180</dep>
        </entry>
        <entry>
          <ind>0.2</ind>
          <dep>-180</dep>
        </entry>
        <entry>
          <ind>1.0</ind>
          <dep>0.0</dep>
        </entry>
      </interpolation>
      <axis>
        <x1-m>1.095</x1-m>
        <y1-m>0.278</y1-m>
        <z1-m>-0.943</z1-m>
        <x2-m>1.31</x2-m>
        <y2-m>0.55</y2-m>
        <z2-m>-1.0</z2-m>
      </axis>
    </animation>
  </PropertyList>
