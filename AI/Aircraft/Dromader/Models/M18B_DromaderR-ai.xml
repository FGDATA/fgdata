<?xml version="1.0"?>


<PropertyList>

	<path>M18B_DromaderR.ac</path>
	
	<offsets>
      <pitch-deg>11.9</pitch-deg>  
	  <z-m>1.88</z-m>
    </offsets>
	
	<animation>
	  <type>rotate</type>
	  <property>velocities/true-airspeed-kt</property>
	  <interpolation>
		<entry><ind>0</ind><dep>0.0</dep></entry>
		<entry><ind>20</ind><dep>0.0</dep></entry>
		<entry><ind>30</ind><dep>11.9</dep></entry>
		<entry><ind>60</ind><dep>11.9</dep></entry>
	  </interpolation>
	  <center>
	   <x-m>0.0</x-m>
	   <y-m>0.0</y-m>
	   <z-m>-15</z-m>
	  </center>
	  <axis>
	   <x>0.0</x>
	   <y>-.5</y>
	   <z>0.0</z>
	  </axis>
	</animation>
 
<!--
<animation>
	<type>material</type>
		<texture-prop>texture</texture-prop>
		<texture>Textures/LiveryDefault.png</texture>
</animation>
-->

<animation>
	<type>select</type>
		<object-name>Aspa1a</object-name>
		<object-name>Aspa1b</object-name>
		<object-name>Aspa1c</object-name>
		<object-name>Aspa2a</object-name>
		<object-name>Aspa2b</object-name>
		<object-name>Aspa2c</object-name>
		<object-name>Aspa3a</object-name>
		<object-name>Aspa3b</object-name>
		<object-name>Aspa3c</object-name>
		<object-name>Aspa4a</object-name>
		<object-name>Aspa4b</object-name>
		<object-name>Aspa4c</object-name>
	<condition>
		<less-than>
			<property>velocities/true-airspeed-kt</property>
			<value>101</value>
		</less-than>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>GiroHelice</object-name>
	<condition>
		<greater-than>
			<property>velocities/true-airspeed-kt</property>
			<value>14</value>
		</greater-than>
	</condition>
 </animation>

 <animation>
	<type>spin</type>
		<object-name>Aspa1a</object-name>
		<object-name>Aspa1b</object-name>
		<object-name>Aspa1c</object-name>
		<object-name>Aspa2a</object-name>
		<object-name>Aspa2b</object-name>
		<object-name>Aspa2c</object-name>
		<object-name>Aspa3a</object-name>
		<object-name>Aspa3b</object-name>
		<object-name>Aspa3c</object-name>
		<object-name>Aspa4a</object-name>
		<object-name>Aspa4b</object-name>
		<object-name>Aspa4c</object-name>
		<object-name>Eje02</object-name>
			<property>velocities/true-airspeed-kt</property>
		<factor>-1</factor>
		<center>
			<x-m>-2.40</x-m>
			<y-m>-0.0</y-m>
			<z-m>-0.0</z-m>
		</center>
		<axis>
			<x>1.0</x>
			<y>0.0</y>
			<z>0.0</z>
		</axis>
 </animation>

 <animation>
	<type>spin</type>
		<object-name>GiroHelice</object-name>
	<property>velocities/true-airspeed-kt</property>
		<factor>-.01</factor>
		<center>
			<x-m>1.15114</x-m>
			<y-m>0</y-m>
			<z-m>-0.0125287</z-m>
		</center>
		<axis>
			<x>1.0</x>
			<y>0.0</y>
			<z>0.0</z>
		</axis>
 </animation>
 
<!-- Rotacion de las ruedas -->
   <animation>
        <type>spin</type>
        <object-name>RuedaTrasera</object-name>
        <property>velocities/true-airspeed-kt</property>
        <factor>12</factor>
        <center>
            <x-m>5.38462</x-m>
            <y-m>0.00</y-m>
            <z-m>-0.55425</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>RuedaIZQ</object-name>
        <property>velocities/true-airspeed-kt</property>
        <factor>12</factor>
        <center>
            <x-m>-0.5133</x-m>
            <y-m>-1.88265</y-m>
            <z-m>-1.63371</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>RuedaDER</object-name>
        <property>velocities/true-airspeed-kt</property>
        <factor>12</factor>
        <center>
            <x-m>-0.513301</x-m>
            <y-m> 1.71131</y-m>
            <z-m>-1.63371</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>


<!-- *****************************
	  Luces (lights)
****************************** -->

<!-- Baliza  *Beacon* -->
	
  <model>  <!-- Beacon inferior -->
	<name>Beacon1</name>
    <path>Lights/Beacon.xml</path>
    <offsets>
      <x-m>-1.08188</x-m>
      <y-m>  0.000 </y-m>
      <z-m>-1.06281</z-m>
    </offsets>
  </model>

  <model>   <!-- Beacon superior -->
	<name>Beacon2</name>
    <path>Lights/Beacon.xml</path>
    <offsets>
      <x-m> 2.68472</x-m>
      <y-m>  0.000 </y-m>
      <z-m> 1.90015</z-m>
    </offsets>
  </model>
 
   <model>
	<name>leftbeaconflash</name>
    <path>Lights/flash.xml</path>
    <offsets>
      <x-m>-1.08188</x-m>
      <y-m>  0.000 </y-m>
      <z-m>-1.06281</z-m>
    </offsets>
  </model>
  
  <model>
	<name>rightbeaconflash</name>
    <path>Lights/flash.xml</path>
    <offsets>
      <x-m> 2.68472</x-m>
      <y-m>  0.000 </y-m>
      <z-m> 1.90015</z-m>
    </offsets>
  </model>
 
  <animation>
    <name>Beacons</name>
    <object-name>Beacon1</object-name>
    <object-name>Beacon2</object-name>
  </animation>

  <animation>
    <name>BeaconsFlash</name>
    <object-name>leftbeaconflash</object-name>
    <object-name>rightbeaconflash</object-name>
  </animation>
  
   <animation>
	  <name>beacon_strobes</name>
	  <type>timed</type>
	  <object-name>Beacons</object-name>
	  <object-name>BeaconsFlash</object-name>
	  <use-personality type="bool">true</use-personality>
		  <branch-duration-sec>0.1</branch-duration-sec>
		  <branch-duration-sec>1.0</branch-duration-sec>
  </animation>
 
  <animation>
    <type>select</type>
    <object-name>beacon_strobes</object-name>
    <condition>
      <and>
		<greater-than>
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
		</greater-than>
		<greater-than>
			<property>velocities/true-airspeed-kt</property>
			<value>0</value>
		</greater-than>
	  </and>
    </condition>
  </animation>

