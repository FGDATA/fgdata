<?xml version="1.0"?>

  <PropertyList> 
    <path>typhoon-ai.ac</path>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/eurofighter/Models/Liveries", 10);
    </load>
    <unload>
      livery_update.stop();
    </unload>
  </nasal>


<!-- nose gear -->
    <animation>
  <type>rotate</type>
  <object-name>nosegeardoor</object-name>
  <property>gear/gear[0]/position-norm</property>
      <interpolation> 
        <entry>
          <ind>1</ind>
          <dep>90</dep>
        </entry>
        <entry>
          <ind>0.7</ind>
          <dep>90</dep>
        </entry>
		<entry>
          <ind>0</ind>
          <dep>0</dep>
        </entry>
      </interpolation>
  <center>
   <x-m>6.581</x-m>
   <y-m>0.181</y-m>
   <z-m>-0.755</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
    <animation>
  <type>rotate</type>
  <object-name>nosegear.1</object-name>
  <object-name>nosegear.2</object-name>
  <object-name>nosewheel</object-name>
  <object-name>nosewheel.1</object-name>
  <object-name>nosewheel.2</object-name>
  <object-name>nosewheel.3</object-name>
  <object-name>nosewheel.4</object-name>
  <property>gear/gear[0]/position-norm</property>
      <interpolation> 
        <entry>
          <ind>0</ind>
          <dep>-110</dep>
        </entry>
        <entry>
          <ind>0.3</ind>
          <dep>-110</dep>
        </entry>
		<entry>
          <ind>1</ind>
          <dep>0</dep>
        </entry>
      </interpolation>
  <center>
   <x-m>5.591</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.614</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

