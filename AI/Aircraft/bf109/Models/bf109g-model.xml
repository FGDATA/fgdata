<?xml version="1.0"?>

<!-- Messerschmitt Bf 109 G14 by Detlef Faber -->

<PropertyList>

 <path>bf109g-ai.ac</path>
 <offsets>
   <z-m>0.0</z-m>
 </offsets>
<!--
