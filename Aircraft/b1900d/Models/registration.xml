<?xml version="1.0"?>
<PropertyList>
  <params>
    <size>1.0</size>
    <alignment>yz-plane</alignment>
    <color>
     <red   type="double">0.0</red>
     <green type="double">0.0</green>
     <blue  type="double">0.0</blue>
    </color>
  </params>

  <text>
    <alignment>center-center</alignment>
    <axis-alignment alias="../../params/alignment"/>
    <type type="string">text-value</type>
    <property type="string">sim/multiplay/generic/string[0]</property>
    <format type="string">%.7s</format>
    <font>arial_black.txf</font>
    <character-size alias="../../params/size"/>
    <character-aspect-ratio type="double">2.0</character-aspect-ratio>
    <draw-alignment type="bool">false</draw-alignment>
    <font-resolution>
      <width type="int">128</width>
      <height type="int">128</height>
    </font-resolution>
  </text>

  <animation>
    <type>material</type>
    <emission>
      <red>0</red>
	  <green>0</green>
	  <blue>0</blue>
    </emission>
    <ambient>
      <red alias="../../../params/color/red"/>
      <green alias="../../../params/color/green"/>
      <blue alias="../../../params/color/blue"/>
    </ambient>
    <diffuse>
      <red alias="../../../params/color/red"/>
      <green alias="../../../params/color/green"/>
      <blue alias="../../../params/color/blue"/>
    </diffuse>
    <specular>
      <red alias="../../../params/color/red"/>
      <green alias="../../../params/color/green"/>
      <blue alias="../../../params/color/blue"/>
    </specular>
  </animation>

</PropertyList>
