<?xml version="1.0"?>

<PropertyList>
  <path>egt.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>EGT.needle</object-name>
    <property>/instrumentation/egt/egt-degc</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>500</ind><dep>0</dep></entry>
      <entry><ind>900</ind><dep>80</dep></entry>
      <entry><ind>2000</ind><dep>80</dep></entry>
      </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.023</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
</PropertyList>
