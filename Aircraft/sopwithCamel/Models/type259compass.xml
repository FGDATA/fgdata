<?xml version="1.0"?>
<PropertyList> 

	<path>type259compass.ac</path>
	<texture-path>Textures</texture-path>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>liquid_face</object-name>
    <object-name>clear_face</object-name>
  </effect>

  <animation> 
		<type>rotate</type>
		<object-name>dial</object-name>
		<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
		<axis> 
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<center>
			<x-m>-0.000751258</x-m>
			<y-m>0.0</y-m>
			<z-m>0.00168722</z-m>
		</center>
	</animation>
	<animation> 
		<type>rotate</type>
		<object-name>face</object-name>
		<property>orientation/roll-deg</property>
		<factor>1</factor>
		<axis> 
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation> 
		<type>select</type>
		<object-name>screws</object-name>
		<condition>
			<property>sim/model/camel/extra-details</property>
		</condition>
	</animation> 

		
	<!-- *** LoD *** -->
	<!--<animation> 
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>-->
</PropertyList>
