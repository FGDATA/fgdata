<?xml version="1.0"?>

<PropertyList>
    
    <path>HallASI.ac</path>
    
    <animation>
        <type>noshadow</type>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>Indicator</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>70.2</ind>
                <dep>0.2</dep>
            </entry>
        </interpolation>
            
    </animation>
    
    <animation>
      <type>range</type>
      <object-name>Axle</object-name>
      <min-m>0</min-m>
      <max-m>2</max-m>
    </animation>

    <animation>
      <type>range</type>
      <object-name>Tube</object-name>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>

    <animation>
      <type>range</type>
      <object-name>Indicator</object-name>
      <min-m>0</min-m>
      <max-m>10</max-m>
    </animation>

    
</PropertyList>



