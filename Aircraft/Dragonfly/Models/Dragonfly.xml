<?xml version="1.0"?>

<PropertyList>

    <path>Dragonfly.ac</path>

    <model>
        <path>Aircraft/Dragonfly/Models/Banner.xml</path>
        <name>Banner</name>
        <condition>
          <property>sim/model/banner-on-hook</property>
        </condition>
        <offsets>
            <x-m>3.65</x-m>
            <y-m>0</y-m>
            <z-m>-0.124</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
      <type>noshadow</type>
      <object-name>Banner</object-name>
      <condition>
        <not>
          <property>sim/model/banner-on-hook</property>
        </not>
      </condition>
    </animation>

    <model>
        <path>Aircraft/Instruments-3d/alt/alt.xml</path>
        <name>Altimeter</name>
        <offsets>
            <x-m>-0.87157</x-m>
            <y-m>-0.063903</y-m>
            <z-m>0.699448</z-m>
            <pitch-deg>-15</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>Aircraft/Instruments-3d/vsi/vsi.xml</path>
        <name>Altimeter</name>
        <offsets>
            <x-m>-0.87157</x-m>
            <y-m>0.063903</y-m>
            <z-m>0.699448</z-m>
            <pitch-deg>-15</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>Aircraft/Dragonfly/Models/RPM.xml</path>
        <name>RPM</name>
        <offsets>
            <x-m>-0.853907</x-m>
            <y-m>0.063903</y-m>
            <z-m>0.618526</z-m>
            <pitch-deg>-15</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>Aircraft/Dragonfly/Models/Airspeed.xml</path>
        <name>Airspeed</name>
        <offsets>
            <x-m>-0.853907</x-m>
            <y-m>-0.063903</y-m>
            <z-m>0.618526</z-m>
            <pitch-deg>-15</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>Aircraft/Dragonfly/Models/HallASI.xml</path>
        <name>AirspeedIndicator</name>
        <offsets>
            <x-m>-0.922</x-m>
            <y-m>0.130</y-m>
            <z-m>0.75</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/Dragonfly/Models/Compass.xml</path>
        <name>Compass</name>
        <offsets>
            <x-m>-0.918</x-m>
            <y-m>0.055</y-m>
            <z-m>0.7614</z-m>
        </offsets>
    </model>


    <animation>
        <type>rotate</type>
        <object-name>Elevator</object-name>
        <property>surface-positions/elevator-pos-norm</property>
        <factor>30.0</factor>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>3.83843</x-m>
            <y-m>0</y-m>
            <z-m>0.50</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Stick.Front</object-name>
        <property>controls/flight/elevator</property>
        <factor>20.0</factor>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>-0.76257</x-m>
            <y-m>0</y-m>
            <z-m>0.369003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Stick.Rear</object-name>
        <property>controls/flight/elevator</property>
        <factor>20.0</factor>
        <axis>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.45843</x-m>
            <y-m>0</y-m>
            <z-m>0.267003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Stick.Front</object-name>
        <property>controls/flight/aileron</property>
        <factor>20.0</factor>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>-0.76257</x-m>
            <y-m>0</y-m>
            <z-m>0.369003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Stick.Rear</object-name>
        <property>controls/flight/aileron</property>
        <factor>20.0</factor>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.45843</x-m>
            <y-m>0</y-m>
            <z-m>0.267003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rudder.Left.Front</object-name>
        <property>controls/flight/rudder</property>
        <factor>20.0</factor>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>-1.30157</x-m>
            <y-m>0</y-m>
            <z-m>0.420003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rudder.Right.Front</object-name>
        <property>controls/flight/rudder</property>
        <factor>-20.0</factor>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>-1.30157</x-m>
            <y-m>0</y-m>
            <z-m>0.420003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rudder</object-name>
        <property>surface-positions/rudder-pos-norm</property>
        <factor>20.0</factor>
        <axis>
            <x1-m>3.66943</x1-m>
            <y1-m>0.0</y1-m>
            <z1-m>0.267003</z1-m>
            <x2-m>4.26343</x2-m>
            <y2-m>0.0</y2-m>
            <z2-m>1.619003</z2-m>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Gear.Left</object-name>
        <property>gear/gear[0]/compression-norm</property>
        <factor>-20.0</factor>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.02582</x-m>
            <y-m>-0.249348</y-m>
            <z-m>0.258682</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Gear.Right</object-name>
        <property>gear/gear[1]/compression-norm</property>
        <factor>20.0</factor>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.02582</x-m>
            <y-m>0.249348</y-m>
            <z-m>0.258682</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Gear.Tail</object-name>
        <property>gear/gear[2]/steering-norm</property>
        <factor>60.0</factor>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
        <center>
            <x-m>3.57731</x-m>
            <y-m>0</y-m>
            <z-m>0.162196</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Throttle</object-name>
        <property>controls/engines/engine[0]/throttle</property>
        <factor>-60.0</factor>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>-0.503904</x-m>
            <y-m>0</y-m>
            <z-m>0.320428</z-m>
        </center>
    </animation>
