        </center>
    </animation>
    <animation>
        <type>select</type>
        <object-name>PropBlades</object-name>
        <condition>
            <less-than>
                <property>engines/engine[0]/rpm</property>
                <value>400</value>
            </less-than>
        </condition>
    </animation>



