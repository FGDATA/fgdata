<?xml version="1.0"?>

<PropertyList>

	<name>ASI</name>
	<path>asi.ac</path>

	<animation>
		<type>select</type>
		<object-name>asi</object-name>
		<condition>
			<equals>
				<property>sim/current-view/name</property>
				<value>Cockpit View</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>face</object-name>
		<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>40</ind><dep>16</dep></entry>
			<entry><ind>75</ind><dep>31</dep></entry>
			<entry><ind>100</ind><dep>45</dep></entry>
			<entry><ind>117</ind><dep>61</dep></entry>
			<entry><ind>135</ind><dep>75</dep></entry>
			<entry><ind>150</ind><dep>90</dep></entry>
			<entry><ind>170</ind><dep>104</dep></entry>
			<entry><ind>185</ind><dep>120</dep></entry>
			<entry><ind>200</ind><dep>134</dep></entry>
			<entry><ind>250</ind><dep>156</dep></entry>
			<entry><ind>300</ind><dep>180</dep></entry>
			<entry><ind>350</ind><dep>204</dep></entry>
			<entry><ind>400</ind><dep>224</dep></entry>
			<entry><ind>500</ind><dep>251</dep></entry>
			<entry><ind>600</ind><dep>280</dep></entry>
			<entry><ind>700</ind><dep>305</dep></entry>
			<entry><ind>800</ind><dep>332</dep></entry>
			<entry><ind>850</ind><dep>345</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>safe-speed-bug</object-name>
		<property>sim/model/f-14b/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>40</ind><dep>16</dep></entry>
			<entry><ind>75</ind><dep>31</dep></entry>
			<entry><ind>100</ind><dep>45</dep></entry>
			<entry><ind>117</ind><dep>61</dep></entry>
			<entry><ind>135</ind><dep>75</dep></entry>
			<entry><ind>150</ind><dep>90</dep></entry>
			<entry><ind>170</ind><dep>104</dep></entry>
			<entry><ind>185</ind><dep>120</dep></entry>
			<entry><ind>200</ind><dep>134</dep></entry>
			<entry><ind>250</ind><dep>156</dep></entry>
			<entry><ind>300</ind><dep>180</dep></entry>
			<entry><ind>350</ind><dep>204</dep></entry>
			<entry><ind>400</ind><dep>224</dep></entry>
			<entry><ind>500</ind><dep>251</dep></entry>
			<entry><ind>600</ind><dep>280</dep></entry>
			<entry><ind>700</ind><dep>305</dep></entry>
			<entry><ind>800</ind><dep>332</dep></entry>
			<entry><ind>850</ind><dep>345</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>mach-scale</object-name>
		<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>40</ind><dep>16</dep></entry>
			<entry><ind>75</ind><dep>31</dep></entry>
			<entry><ind>100</ind><dep>45</dep></entry>
			<entry><ind>117</ind><dep>61</dep></entry>
			<entry><ind>135</ind><dep>75</dep></entry>
			<entry><ind>150</ind><dep>90</dep></entry>
			<entry><ind>170</ind><dep>104</dep></entry>
			<entry><ind>185</ind><dep>120</dep></entry>
			<entry><ind>200</ind><dep>134</dep></entry>
			<entry><ind>250</ind><dep>156</dep></entry>
			<entry><ind>300</ind><dep>180</dep></entry>
			<entry><ind>350</ind><dep>204</dep></entry>
			<entry><ind>400</ind><dep>224</dep></entry>
			<entry><ind>500</ind><dep>251</dep></entry>
			<entry><ind>600</ind><dep>280</dep></entry>
			<entry><ind>700</ind><dep>305</dep></entry>
			<entry><ind>800</ind><dep>332</dep></entry>
			<entry><ind>850</ind><dep>345</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>mach-scale</object-name>
		<property>velocities/mach</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.8</ind><dep>90</dep></entry>
			<entry><ind>1.4</ind><dep>180</dep></entry>
			<entry><ind>2.2</ind><dep>270</dep></entry>
			<entry><ind>2.6</ind><dep>315</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

<!-- Air Speed Indicator's Bug Setting -->
	<animation>
		<name>asi-bug-left-fine</name>
		<type>pick</type>
		<visible>true</visible>
		<object-name>asi-bug-left</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
				<step>-10</step>
				<min>225</min>
			</binding>
		</action>
	</animation>
	<animation>
		<name>asi-bug-left-coarse</name>
		<type>pick</type>
		<visible>true</visible>
		<object-name>asi-bug-left</object-name>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
				<step>-50</step>
				<min>225</min>
			</binding>
		</action>
	</animation>

	<animation>
		<name>asi-bug-right-fine</name>
		<type>pick</type>
		<visible>true</visible>
		<object-name>asi-bug-right</object-name>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
				<step>10</step>
				<max>850</max>
			</binding>
		</action>
	</animation>
	<animation>
		<name>asi-bug-right-coarse</name>
		<type>pick</type>
		<visible>true</visible>
		<object-name>asi-bug-right</object-name>
		<action>
			<button>1</button>
			<repeatable>true</repeatable>
			<interval-sec>0.2</interval-sec>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/f-14b/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
				<step>50</step>
				<max>850</max>
			</binding>
		</action>
	</animation>


	<!-- Lightning -->
	<animation>
		<object-name>mount</object-name>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
				<value>0</value>
			</greater-than>
            <not><property>sim/rendering/rembrandt/enabled</property></not>
        </condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.55</red>
			<green>0.24</green>
			<blue>0.10</blue>
		</emission>
	</animation>
	<animation>
		<object-name>knob</object-name>
		<type>material</type>
		<condition>
			<greater-than>
				<property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
				<value>0</value>
			</greater-than>
<not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.55</red>
			<green>0.24</green>
			<blue>0.10</blue>
		</emission>
	</animation>

