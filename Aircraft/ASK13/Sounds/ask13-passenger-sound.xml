  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Aircraft/ASK13/Sounds/wind-ask13.wav</path>
   <property>velocities/airspeed-kt</property>
   <volume>
    <property>position/altitude-ft</property>
    <factor>-0.000015</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>velocities/airspeed-kt</property>
     <factor>0.015</factor>
     <min>0.03</min>
     <max>5</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.002</factor>
    <offset>1</offset>
   </pitch> 
  </wind>
