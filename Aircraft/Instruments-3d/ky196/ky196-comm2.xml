                <property>instrumentation/comm[1]/serviceable</property>
                <value>1</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>KY196.volume</object-name>
        <action>
            <button>4</button>
            <binding>
				<condition>
					<greater-than>
						<property>instrumentation/comm[1]/volume</property>
						<value>0</value>
					</greater-than>
				</condition>
                <command>property-adjust</command>
                <property>instrumentation/comm[1]/volume</property>
