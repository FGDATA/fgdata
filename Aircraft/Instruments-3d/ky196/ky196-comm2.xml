            <z>0</z>
        </axis>
    </animation>
 
    <animation>
        <type>textranslate</type>
        <object-name>stby.104</object-name>
        <property>instrumentation/comm[1]/frequencies/standby-mhz</property>
        <factor>1.0</factor>
        <step>0.1</step>
        <bias>0.005</bias>
