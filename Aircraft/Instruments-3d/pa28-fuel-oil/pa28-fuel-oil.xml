   <z-m>-0.022</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RightFuelNeedle</object-name>
  <property>/consumables/fuel/tank[1]/level-gal_us</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-29</dep>
   </entry>
   <entry>
    <ind>5</ind>
    <dep>-18</dep>
   </entry>
   <entry>
