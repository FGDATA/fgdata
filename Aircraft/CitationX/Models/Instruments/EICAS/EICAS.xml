<PropertyList>

<path>eicas.ac</path>

   <panel>
        <path>Aircraft/CitationX/Models/Instruments/EICAS/EICAS-panel.xml</path>
        <bottom-left>
            <x-m>0.003</x-m>
            <y-m>-0.076</y-m>
            <z-m>-0.077</z-m>
        </bottom-left>
        <bottom-right>
            <x-m>0.003</x-m>
            <y-m>0.076</y-m>
            <z-m>-0.077</z-m>
        </bottom-right>
        <top-left>
            <x-m>0.003</x-m>
            <y-m>-0.076</y-m>
            <z-m>0.106</z-m>
        </top-left>
    </panel>

<animation>
    <type>select</type>
    <object-name>EICAS.norm</object-name>
    <object-name>Flapneedle</object-name>
    <object-name>STABneedle</object-name>
    <object-name>LH-N1</object-name>
    <object-name>RH-N1</object-name>
    <object-name>LH-ITT</object-name>
    <object-name>RH-ITT</object-name>
    <object-name>oiltemp.001</object-name>
    <object-name>oiltemp.002</object-name>
    <object-name>oilpsi.001</object-name>
    <object-name>oilpsi.002</object-name>
            <condition>
                <and>
                    <property>instrumentation/primus1000/eicas/serviceable</property>
                    <greater-than>
                        <property>systems/electrical/outputs/efis</property>
                        <value>15</value>
                    </greater-than>
                </and>
            </condition>
</animation>


<animation>
    <type>select</type>
    <object-name>gear0.annun</object-name>
    <condition>
        <and>
            <equals>
            <property>gear/gear[0]/position-norm</property>
            <value>1.0</value>
            </equals>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
        </and>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>gear1.annun</object-name>
    <condition>
        <and>
            <equals>
                <property>gear/gear[1]/position-norm</property>
                <value>1.0</value>
            </equals>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
        </and>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>gear2.annun</object-name>
    <condition>
        <and>
            <equals>
                <property>gear/gear[2]/position-norm</property>
