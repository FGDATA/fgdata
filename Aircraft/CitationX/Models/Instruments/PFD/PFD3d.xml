<?xml version="1.0"?>
<PropertyList>

<path>pfd3d.ac</path>

    <animation>
        <name>ias ladder</name>
        <type>texmultiple</type>
        <object-name>iasladder</object-name>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>100.000</ind><dep>0.4536</dep></entry>
                <entry><ind>100.001</ind><dep>0.0</dep></entry>
                <entry><ind>180.000</ind><dep>0.4536</dep></entry>
                <entry><ind>180.001</ind><dep>0.0</dep></entry>
                <entry><ind>270.000</ind><dep>0.4536</dep></entry>
                <entry><ind>270.001</ind><dep>0.0</dep></entry>
                <entry><ind>360.000</ind><dep>0.4536</dep></entry>
                <entry><ind>360.001</ind><dep>0.0</dep></entry>
                <entry><ind>450.000</ind><dep>0.4536</dep></entry>
                <entry><ind>450.001</ind><dep>0.0</dep></entry>
                <entry><ind>540.000</ind><dep>0.4536</dep></entry>
            </interpolation>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>100.000</ind><dep>0.0</dep></entry>
                <entry><ind>100.001</ind><dep>0.11</dep></entry>
                <entry><ind>180.000</ind><dep>0.11</dep></entry>
                <entry><ind>180.001</ind><dep>0.22</dep></entry>
                <entry><ind>270.000</ind><dep>0.22</dep></entry>
                <entry><ind>270.001</ind><dep>0.33</dep></entry>
                <entry><ind>360.000</ind><dep>0.33</dep></entry>
                <entry><ind>360.001</ind><dep>0.44</dep></entry>
                <entry><ind>450.000</ind><dep>0.44</dep></entry>
                <entry><ind>450.001</ind><dep>0.55</dep></entry>
                <entry><ind>540.000</ind><dep>0.55</dep></entry>
            </interpolation>
            <axis>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </axis>
        </transform>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ias.d1</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ias.d2</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ias.d3</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <factor>0.1</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <name>alt ladder</name>
        <type>texmultiple</type>
        <object-name>altladder</object-name>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/altimeter/indicated-altitude-ft</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>5000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>5000.001</ind><dep>0.0</dep></entry>
                <entry><ind>10000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>10000.001</ind><dep>0.0</dep></entry>
                <entry><ind>15000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>15000.001</ind><dep>0.0</dep></entry>
                <entry><ind>20000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>20000.001</ind><dep>0.0</dep></entry>
                <entry><ind>25000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>25000.001</ind><dep>0.0</dep></entry>
                <entry><ind>30000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>30000.001</ind><dep>0.0</dep></entry>
                <entry><ind>35000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>35000.001</ind><dep>0.0</dep></entry>
                <entry><ind>40000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>40000.001</ind><dep>0.0</dep></entry>
                <entry><ind>45000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>45000.001</ind><dep>0.0</dep></entry>
                <entry><ind>50000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>50000.001</ind><dep>0.0</dep></entry>
                <entry><ind>55000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>55000.001</ind><dep>0.0</dep></entry>
                <entry><ind>60000.000</ind><dep>0.7570</dep></entry>
                <entry><ind>60000.001</ind><dep>0.0</dep></entry>
                <entry><ind>65000.000</ind><dep>0.7570</dep></entry>
            </interpolation>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/altimeter/indicated-altitude-ft</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.000</dep></entry>
                <entry><ind>5000.000</ind><dep>0.000</dep></entry>
                <entry><ind>5000.001</ind><dep>0.075</dep></entry>
                <entry><ind>10000.000</ind><dep>0.075</dep></entry>
                <entry><ind>10000.001</ind><dep>0.150</dep></entry>
                <entry><ind>15000.000</ind><dep>0.150</dep></entry>
                <entry><ind>15000.001</ind><dep>0.225</dep></entry>
                <entry><ind>20000.000</ind><dep>0.225</dep></entry>
                <entry><ind>20000.001</ind><dep>0.300</dep></entry>
                <entry><ind>25000.000</ind><dep>0.300</dep></entry>
                <entry><ind>250000.001</ind><dep>0.375</dep></entry>
                <entry><ind>30000.000</ind><dep>0.375</dep></entry>
                <entry><ind>30000.001</ind><dep>0.450</dep></entry>
                <entry><ind>35000.000</ind><dep>0.450</dep></entry>
                <entry><ind>35000.001</ind><dep>0.525</dep></entry>
                <entry><ind>40000.000</ind><dep>0.525</dep></entry>
                <entry><ind>40000.001</ind><dep>0.600</dep></entry>
                <entry><ind>45000.000</ind><dep>0.600</dep></entry>
                <entry><ind>45000.001</ind><dep>0.675</dep></entry>
                <entry><ind>50000.000</ind><dep>0.675</dep></entry>
                <entry><ind>50000.001</ind><dep>0.750</dep></entry>
                <entry><ind>55000.000</ind><dep>0.750</dep></entry>
                <entry><ind>55000.001</ind><dep>0.825</dep></entry>
                <entry><ind>60000.000</ind><dep>0.825</dep></entry>
                <entry><ind>60000.001</ind><dep>0.900</dep></entry>
            </interpolation>
            <axis>
                <x>-1</x>
                <y>0</y>
                <z>0</z>
            </axis>
        </transform>
    </animation>


    <animation>
        <type>textranslate</type>
        <object-name>alt.d1</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.00001</factor>
        <step>10000</step>
        <bias>0.5</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>alt.d2</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.0001</factor>
        <step>1000</step>
        <bias>0.5</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>alt.d3</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.001</factor>
        <step>100</step>
        <bias>0.5</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>alt.d4</object-name>
        <property>instrumentation/altimeter/indicated-altitude-ft</property>
        <factor>0.01</factor>
        <bias>0.5</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <name>Attitude indicator</name>
        <type>texmultiple</type>
        <object-name>att</object-name>
        <transform>
            <subtype>textranslate</subtype>
            <property>orientation/pitch-deg</property>
            <interpolation>
                <entry><ind>-90.0</ind><dep>-0.41</dep></entry>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>90.0</ind><dep>0.41</dep></entry>
            </interpolation>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <subtype>texrotate</subtype>
            <property>orientation/roll-deg</property>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
            <center>
                <x>0.25</x>
                <y>0.5</y>
            </center>
        </transform>
    </animation>

<!-- hsi rose -->

    <animation>
        <type>texrotate</type>
        <object-name>PFD.hsi</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <center>
            <x>0.75</x>
            <y>0.25</y>
        </center>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>hdg.bug</object-name>
        <property>autopilot/internal/heading-bug-error-deg</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>crs.needle</object-name>
        <object-name>crs.deflect</object-name>
        <object-name>to-flag</object-name>
        <object-name>from-flag</object-name>
        <property>autopilot/internal/course-offset</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>to-flag</object-name>
        <condition>
            <property>autopilot/internal/to-flag</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>from-flag</object-name>
        <condition>
            <property>autopilot/internal/from-flag</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/primus2000/sc840/nav1ptr</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-vor</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>1</value>
        </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-adf</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>2</value>
        </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-fms</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>3</value>
        </equals>
        </condition>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>ptr1</object-name>
        <property>autopilot/internal/nav1-pointer</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-vor</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-adf</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>2</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-fms</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>3</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ptr2</object-name>
        <property>autopilot/internal/nav2-pointer</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>crs.deflect</object-name>
        <property>autopilot/internal/cdi</property>
        <interpolation>
            <entry><ind>-45.0</ind><dep>-0.01845</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>45.0</ind><dep>0.01845</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

<!-- end hsi rose -->

    <animation>
        <type>rotate</type>
        <object-name>vsi.needle</object-name>
        <property>autopilot/internal/vert-speed-fpm</property>
        <interpolation>
            <entry><ind>-4000.0</ind><dep>-73.33</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>4000</ind><dep>73.33</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.0825</y-m>
            <z-m>-0.040</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>gs.scale</object-name>
        <object-name>gs.needle</object-name>
        <condition>
            <property>autopilot/internal/gs-valid</property>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>gs.needle</object-name>
        <property>autopilot/internal/gs-deflection</property>
        <interpolation>
            <entry><ind>-1.0</ind><dep>-0.0162</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>1.0</ind><dep>0.0162</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

<!-- Text -->

<model>
        <name>text3d</name>
        <path>Aircraft/CitationX/Models/Instruments/PFD/text3d.xml</path>
        <offsets>
            <x-m>0.0016</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>VSbox</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/altitude</property>
                <value>VS</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Omarker</object-name>
        <condition>
            <property>instrumentation/marker-beacon/outer</property>
         </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Mmarker</object-name>
        <condition>
            <property>instrumentation/marker-beacon/middle</property>
         </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Imarker</object-name>
        <condition>
            <property>instrumentation/marker-beacon/inner</property>
         </condition>
    </animation>

</PropertyList>