 <!--== Instrument override pick animations ================================-->

 <!-- Backup altimeter setting knob. -->
 <animation>
  <type>pick</type>
  <object-name>InhgKnob</object-name>
  <visible>true</visible>
  <action>
   <name>increase calibrate</name>
   <button>0</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>instrumentation/altimeter[1]/setting-inhg</property>
      <step>0.01</step>
      <min>26.0</min>
      <max>33.0</max>
      <wrap>false</wrap>
    </binding>
   </action>
   <action>
    <name>decrease calibrate</name>
    <button>1</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>instrumentation/altimeter[1]/setting-inhg</property>
      <step>-0.01</step>
      <min>26.0</min>
      <max>33.0</max>
      <wrap>false</wrap>
    </binding>
  </action>
 </animation>

 <!-- Chronometer. -->
 <animation>
  <type>pick</type>
  <object-name>SEL.btn</object-name>
  <visible>true</visible>
  <action>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/clock/m877/mode</property>
    <step>1</step>
    <min>0</min>
    <max>4</max>
    <wrap>true</wrap>
   </binding>
  </action>
 </animation>

 <!-- LOD for the model as a whole =========================================-->
 <animation>
