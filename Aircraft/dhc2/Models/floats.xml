<?xml version="1.0"?>

<PropertyList>

  <path>floats.ac</path>

  <model>
    <path>Aircraft/dhc2/Models/Effects/left-spray.xml</path>
    <name>spray-left</name>
    <offsets>
      <x-m>-1.0</x-m>
      <y-m>-1.5</y-m>
      <z-m>-2.0</z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/dhc2/Models/Effects/right-spray.xml</path>
    <name>spray-right</name>
    <offsets>
      <x-m>-1.0</x-m>
      <y-m>1.5</y-m>
      <z-m>-2.0</z-m>
    </offsets>
  </model>

  <model>
    <condition>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
    <name>Fshadow</name>
    <path>Aircraft/dhc2/Models/shadowF.xml</path>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>

  <animation>
    <type>material</type>
    <object-name>Floats</object-name>
    <object-name>LHrudder</object-name>
    <object-name>RHrudder</object-name>
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>paint1.png</texture>
  </animation>

  <effect>
    <inherits-from>Aircraft/dhc2/Models/Effects/skin-reflect</inherits-from>
    <object-name>Floats</object-name>
    <object-name>LHrudder</object-name>
    <object-name>RHrudder</object-name>
  </effect>

  <animation>
    <name>LeftFrontGear</name>
    <object-name>Lstrut.001</object-name>
    <object-name>Ldamper</object-name>
    <object-name>LFwheel</object-name>
  </animation>

  <animation>
    <name>RightFrontGear</name>
    <object-name>Rstrut.001</object-name>
    <object-name>Rdamper</object-name>
    <object-name>RFwheel</object-name>
  </animation>

  <!--    Retractable Gear    -->

  <animation>
    <type>rotate</type>
    <condition>
      <property>controls/gear/gear-down</property>
    </condition>
    <object-name>Ldamper</object-name>
    <object-name>LFwheel</object-name>
    <property>gear/gear[0]/compression-m</property>
    <factor>400</factor>
    <center>
      <x-m>-3.592</x-m>
      <y-m>1.434</y-m>
      <z-m>-2.209</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LeftFrontGear</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>0.320</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-3.553</x-m>
      <y-m>1.430</y-m>
      <z-m>-1.901</z-m>
    </center>
    <axis>
      <x>0.342</x>
      <y>0</y>
      <z>0.940</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Lstrut.002</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>0.115</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-3.553</x-m>
      <y-m>1.430</y-m>
      <z-m>-1.901</z-m>
    </center>
    <axis>
      <x>0.342</x>
      <y>0</y>
      <z>0.940</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <condition>
      <property>controls/gear/gear-down</property>
    </condition>
    <object-name>Rdamper</object-name>
    <object-name>RFwheel</object-name>
    <property>gear/gear[1]/compression-m</property>
    <factor>400</factor>
    <center>
      <x-m>-3.592</x-m>
      <y-m>-1.434</y-m>
      <z-m>-2.209</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>RightFrontGear</object-name>
    <property>gear/gear[1]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>0.320</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-3.553</x-m>
      <y-m>-1.430</y-m>
      <z-m>-1.901</z-m>
    </center>
    <axis>
      <x>0.342</x>
      <y>0</y>
      <z>0.940</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>Rstrut.002</object-name>
    <property>gear/gear[1]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>0.115</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-3.553</x-m>
      <y-m>-1.430</y-m>
      <z-m>-1.901</z-m>
    </center>
    <axis>
      <x>0.342</x>
      <y>0</y>
      <z>0.940</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LRwheel</object-name>
    <property>gear/gear[2]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>-0.34</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.216</x-m>
      <y-m>1.33</y-m>
      <z-m>-2.33</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <condition>
      <property>controls/gear/gear-down</property>
    </condition>
    <object-name>LRwheel</object-name>
    <property>gear/gear[2]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>0.216</x-m>
      <y-m>1.33</y-m>
      <z-m>-2.33</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>RRwheel</object-name>
    <property>gear/gear[3]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>-0.34</dep>
      </entry>
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.216</x-m>
      <y-m>-1.33</y-m>
      <z-m>-2.33</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <condition>
      <property>controls/gear/gear-down</property>
    </condition>
    <object-name>RRwheel</object-name>
    <property>gear/gear[3]/compression-m</property>
    <factor>1.0</factor>
    <center>
      <x-m>0.216</x-m>
      <y-m>-1.33</y-m>
      <z-m>-2.33</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RHrudder</object-name>
    <object-name>RHrudderArm</object-name>
    <property>controls/gear/steering</property>
    <factor>-20</factor>
    <center>
      <x-m>3.360</x-m>
      <y-m>1.434</y-m>
      <z-m>-1.632</z-m>
    </center>
    <axis>
      <x>-0.087</x>
      <y>0</y>
      <z>0.996</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RHrudder</object-name>
    <property>controls/gear/water-rudder-down</property>
    <factor>90</factor>
    <center>
      <x-m>3.360</x-m>
      <y-m>1.434</y-m>
      <z-m>-1.632</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LHrudder</object-name>
    <object-name>LHrudderArm</object-name>
    <property>controls/gear/steering</property>
    <factor>-20</factor>
    <center>
      <x-m>3.360</x-m>
      <y-m>-1.434</y-m>
      <z-m>-1.632</z-m>
    </center>
    <axis>
      <x>-0.087</x>
      <y>0</y>
      <z>0.996</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LHrudder</object-name>
    <property>controls/gear/water-rudder-down</property>
    <factor>90</factor>
    <center>
      <x-m>3.360</x-m>
      <y-m>-1.434</y-m>
      <z-m>-1.632</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>