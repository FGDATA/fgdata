<?xml version="1.0"?>

<PropertyList>

<path>manifold-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>MANneedle</object-name>
        <object-name>MANface</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>MANneedle</object-name>
        <property>/engines/engine/mp-osi</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>10.0</ind><dep>20.0</dep></entry>
            <entry><ind>30.0</ind><dep>180.0</dep></entry>
            <entry><ind>50.0</ind><dep>337.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>
