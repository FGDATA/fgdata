                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>KN53.vol</object-name>
        <action>
            <button>4</button>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav/volume</property>
                <step>-0.05</step>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>KN53.knob1</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav/frequencies/standby-mhz</property>
                <step>0.05</step>
                <min>108.00</min>
                <max>118.00</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>KN53.knob1</object-name>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav/frequencies/standby-mhz</property>
                <step>-0.05</step>
                <min>108.00</min>
                <max>118.00</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>KN53.knob1</object-name>
        <action>
            <button>3</button>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav/frequencies/standby-mhz</property>
