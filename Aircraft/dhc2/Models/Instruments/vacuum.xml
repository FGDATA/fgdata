<?xml version="1.0"?>

<PropertyList>

<path>vac-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>VACface</object-name>
        <object-name>VACneedle</object-name>
         <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>VACneedle</object-name>
        <property>/systems/vacuum/suction-inhg</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>10.0</ind><dep>300.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>