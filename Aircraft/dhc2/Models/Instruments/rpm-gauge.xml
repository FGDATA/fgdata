<?xml version="1.0"?>

<PropertyList>

<path>rpm-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>RPMneedle</object-name>
        <object-name>RPMface</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RPMneedle</object-name>
        <property>/engines/engine/rpm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>500.0</ind><dep>35.0</dep></entry>
            <entry><ind>1000.0</ind><dep>68.0</dep></entry>
            <entry><ind>1500.0</ind><dep>104.0</dep></entry>
            <entry><ind>2000.0</ind><dep>140.0</dep></entry>
            <entry><ind>2500.0</ind><dep>175.0</dep></entry>
            <entry><ind>3000.0</ind><dep>211.0</dep></entry>
            <entry><ind>3500.0</ind><dep>245.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>
