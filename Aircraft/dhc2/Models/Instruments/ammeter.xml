<?xml version="1.0"?>

<PropertyList>

<path>ammeter.ac</path>

    <animation>
        <type>material</type>
        <object-name>AMPgauge.001</object-name>
        <object-name>AMPneedle</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AMPneedle</object-name>
        <property>/systems/electrical/amps</property>
        <interpolation>
            <entry><ind> -60</ind><dep> -50</dep></entry>
            <entry><ind> 0</ind><dep> 0</dep></entry>
            <entry><ind> 60</ind><dep> 50</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.013</z-m>
        </center>
    </animation>

</PropertyList>