<?xml version="1.0"?>

<PropertyList>

    <sim>
        <model>
            <path>Aircraft/dhc2/Models/dhc2.xml</path>
                <livery>
                    <file type="string">TPA</file>
                </livery>
                <cabin>
                    <file type="string">BA</file>
                </cabin>
                <registration>
                    <red type="double">0.0</red>
                    <green type="double">0.0</green>
                    <blue type="double">0.0</blue>
                </registration>
                <display-callsign type="bool">1</display-callsign>
                <shadow-2d type="bool">1</shadow-2d>
        </model>

        <view n="0">
            <name>Cockpit</name>
            <type>lookfrom</type>
            <internal type="bool">true</internal>
            <config>
                <x-offset-m type="double">-0.34</x-offset-m>
                <y-offset-m type="double">0.67</y-offset-m>
                <z-offset-m type="double">-0.64</z-offset-m>
                <pitch-offset-deg>-15</pitch-offset-deg>
                <default-field-of-view-deg type="double">60.0</default-field-of-view-deg>
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                        <heading-max-deg type="double">130</heading-max-deg>
                        <x-offset-max-m type="double">0.22</x-offset-max-m>
                        <x-offset-threshold-deg type="double">75</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">140</heading-max-deg>
                        <x-offset-max-m type="double">0.2</x-offset-max-m>
                        <x-offset-threshold-deg type="double">75</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>

        <chase-distance-m>-20</chase-distance-m>

        <airport>
            <runways>
                <start-offset-m>20</start-offset-m>
            </runways>
        </airport>

        <systems>
            <electrical>
                <path></path>
            </electrical>
        </systems>

        <sound>
            <path>Aircraft/dhc2/Sound/dhc2-sound.xml</path>
            <volume>0.5</volume>
        </sound>

        <instrumentation>
            <path>Aircraft/dhc2/instrumentation.xml</path>
        </instrumentation>

        <allow-toggle-cockpit archive="y">true</allow-toggle-cockpit>
        <yokes-visible type="bool">0</yokes-visible>

        <flaps>
            <setting>0.00</setting> <!-- Cruise -->
            <setting>0.10</setting> <!--  -->
            <setting>0.20</setting> <!-- Climb -->
            <setting>0.30</setting> <!--  -->
            <setting>0.40</setting> <!--  -->
            <setting>0.50</setting> <!--  -->
            <setting>0.60</setting> <!--  -->
            <setting>0.70</setting> <!--  -->
            <setting>0.80</setting> <!--  -->
            <setting>0.90</setting> <!--  -->
            <setting>1.0</setting> <!-- Full -->
        </flaps>

        <weight n="0">
            <name>Crew</name>
            <weight-lb>180</weight-lb> 
            <max-lb>400</max-lb>
            <min-lb>180</min-lb>
        </weight>

        <weight n="1">
            <name>Frt Pax</name>
            <weight-lb>360</weight-lb> 
            <max-lb>500</max-lb>
            <min-lb>0</min-lb>      
        </weight>

        <weight n="2">
            <name>Rr Pax</name>
            <weight-lb>300</weight-lb> 
            <max-lb>500</max-lb>
            <min-lb>0</min-lb>      
        </weight>

        <weight n="3">
            <name>Baggage</name>
            <weight-lb>0</weight-lb> 
            <max-lb>1000</max-lb>
            <min-lb>0</min-lb>      
        </weight>

        <multiplay>
            <generic>
                <string n="0" alias="/sim/aero"/>
                <string n="1" alias="/sim/multiplay/callsign"/>
                <float n="0" alias="/controls/doors/position-norm[0]"/>
                <float n="1" alias="/controls/doors/position-norm[1]"/>
                <float n="2" alias="/controls/doors/position-norm[2]"/>
                <float n="3" alias="/controls/doors/position-norm[3]"/>
                <float n="4" alias="/sim/model/registration/red"/>
                <float n="5" alias="/sim/model/registration/green"/>
                <float n="6" alias="/sim/model/registration/blue"/>
                <float n="7" alias="/position/altitude-agl-ft"/>
                <int n="0" alias="/sim/model/display-callsign"/>
            </generic>
        </multiplay>

    </sim>

    <engines>
        <engine>
            <rpm type="double">0</rpm>
            <magnetos>0</magnetos>
            <running>false</running>
        </engine>
    </engines>

    <controls>
        <lighting>
            <instruments-norm>1.0</instruments-norm>
        </lighting>
        <fuel>
            <switch-position type = "int">-1</switch-position>
        </fuel>
        <gear>
            <brake-parking type="double">1.0</brake-parking>
            <gear-down type="bool">1.0</gear-down>
        </gear>
        <engines>
            <engine>
                <mixture>1.0</mixture>
            </engine>
        </engines>
        <electric>
            <avionics-switch type="bool">1</avionics-switch>
        </electric>
        <doors>
            <open n="0" type="bool">0</open>
            <open n="1" type="bool">0</open>
            <open n="2" type="bool">0</open>
            <open n="3" type="bool">0</open>
            <position-norm n="0" type="double">0</position-norm>
            <position-norm n="1" type="double">0</position-norm>
            <position-norm n="2" type="double">0</position-norm>
            <position-norm n="3" type="double">0</position-norm>
        </doors>
    </controls>

    <instrumentation>
        <kr-87>
            <inputs>
                <volume>0.05</volume>
            </inputs>
        </kr-87>
    </instrumentation>

    <nasal>
        <dhc2>
            <file>Aircraft/dhc2/Nasal/dhc2.nas</file>
            <file>Aircraft/dhc2/Nasal/Electrical.nas</file>
        </dhc2>
    </nasal>

</PropertyList>
