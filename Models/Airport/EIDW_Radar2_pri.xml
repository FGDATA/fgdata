<?xml version="1.0" encoding="iso-8859-1"?> 
<PropertyList>
  <path>EIDW_Radar2_pri.ac</path>
	
  <animation>
    <type>rotate</type>
    <object-name>radardish</object-name>
    <property>/sim/time/elapsed-sec</property>
    <factor>90.0</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>
	
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>1000</max-m>
  </animation>
</PropertyList>