<?xml version="1.0"?>

<PropertyList>

 <path>radar-mil-1.ac</path>


 <animation>
  <type>range</type>
  <object-name>radar</object-name>
  <max-m>18000</max-m>
 </animation>

 <animation>
  <type>range</type>
  <object-name>tower</object-name>
  <max-m>20000</max-m>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>radar</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>-90.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>10.3</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>


</PropertyList>
