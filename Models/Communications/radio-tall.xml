<?xml version="1.0"?>

<PropertyList>

 <path>radio-tall.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>

 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Detailed</object-name>
  <min-m>0</min-m>
  <max-m>1000</max-m>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>Wires.1</object-name>
 </animation>
 
 <animation>
  <type>range</type>
  <object-name>Wires.1</object-name>
  <min-m>0</min-m>
  <max-m>3000</max-m>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>Wires.2</object-name>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Wires.2</object-name>
  <min-m>0</min-m>
  <max-m>3000</max-m>
 </animation>

 <animation>
  <type>range</type>
  <object-name>Bare</object-name>
  <min-m>1000</min-m>
  <max-m>32000</max-m>
 </animation>

 <animation>
  <type>billboard</type>
  <object-name>Bare</object-name>
  <spherical type="bool">false</spherical>
 </animation>

<!--  -->
 <animation>
  <name>StrobesOn</name>
  <object-name>RedLight.011</object-name>
  <object-name>RedLight.031</object-name>
  <object-name>RedLight.051</object-name>
  <object-name>RedLight.071</object-name>
  <object-name>RedLight.091</object-name>
 </animation>

 <animation>
  <name>StrobesOff</name>
  <object-name>RedLight.012</object-name>
  <object-name>RedLight.032</object-name>
  <object-name>RedLight.052</object-name>
  <object-name>RedLight.072</object-name>
  <object-name>RedLight.092</object-name>
 </animation>

 <animation>
  <name>Fixed</name>
  <object-name>RedLight.020</object-name>
  <object-name>RedLight.040</object-name>
  <object-name>RedLight.060</object-name>
  <object-name>RedLight.080</object-name>
  <object-name>RedLight.100</object-name>
 </animation>
 
 <animation>
  <type>timed</type>
  <object-name>StrobesOn</object-name>
  <object-name>StrobesOff</object-name>
  <use-personality type="bool">true</use-personality>
  <branch-duration-sec>
   <random>
    <min>0.9</min>
    <max>1.1</max>
   </random>
  </branch-duration-sec>
  <branch-duration-sec>
   <random>
    <min>0.9</min>
    <max>1.1</max>
   </random>
  </branch-duration-sec>
 </animation>

