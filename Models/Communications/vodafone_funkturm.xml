<?xml version="1.0"?>

<PropertyList>
	<path>vodafone_funkturm.ac</path>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>25000</max-m>
	</animation>

	<rotlichtXparams>
		<light-near>0.4</light-near>
		<light-med>0.8</light-med>
		<light-far>10</light-far>
	</rotlichtXparams>

	<animation>
		<name>rotlichtXGroup</name>
		<object-name>rotlichtX0</object-name>
		<object-name>rotlichtX1</object-name>
		<object-name>rotlichtX2</object-name>
		<object-name>rotlichtX3</object-name>
		<object-name>rotlichtX4</object-name>
	</animation>

	<animation>
		<type>select</type>
		<name>rotlichtXGroupNight</name>
		<object-name>rotlichtXGroup</object-name>
		<condition>
			<greater-than>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.57</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>alpha-test</type>
		<object-name>rotlichtXGroup</object-name>
		<alpha-factor>0.001</alpha-factor>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>rotlichtX0</object-name>
		<offset-m>100.322311</offset-m>
		<axis>
			<x>0</x>
			<y>2e-06</y>
			<z>100.322311</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>rotlichtX1</object-name>
		<offset-m>64.390065</offset-m>
		<axis>
			<x>-3.585277</x>
			<y>3.788234</y>
			<z>64.178467</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>rotlichtX2</object-name>
		<offset-m>64.390065</offset-m>
		<axis>
			<x>3.788234</x>
			<y>3.585277</y>
			<z>64.178467</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>rotlichtX3</object-name>
		<offset-m>64.390065</offset-m>
		<axis>
			<x>-3.788235</x>
			<y>-3.585276</y>
			<z>64.178467</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>rotlichtX4</object-name>
		<offset-m>64.390065</offset-m>
		<axis>
			<x>3.585277</x>
			<y>-3.788234</y>
			<z>64.178467</z>
		</axis>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>rotlichtX0</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>rotlichtX1</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>rotlichtX2</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>rotlichtX3</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>rotlichtX4</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>rotlichtX0</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../rotlichtXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../rotlichtXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../rotlichtXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>rotlichtX1</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../rotlichtXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../rotlichtXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../rotlichtXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>rotlichtX2</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../rotlichtXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../rotlichtXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../rotlichtXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>rotlichtX3</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../rotlichtXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../rotlichtXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../rotlichtXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>rotlichtX4</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../rotlichtXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../rotlichtXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../rotlichtXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

</PropertyList>
