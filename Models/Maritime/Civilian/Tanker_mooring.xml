			<entry>
				<ind>500</ind>
				<dep alias="../../../../bec1Xparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../bec1Xparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>bec1X1</object-name>
		<interpolation>
			<entry>
