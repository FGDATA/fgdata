			<entry>
				<ind>16000</ind>
				<dep alias="../../../../bec1Xparams/light-far"/>
			</entry>
		</interpolation>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>bec1X2</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
