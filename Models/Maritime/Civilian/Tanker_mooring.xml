<?xml version="1.0"?>


<PropertyList>
<path>Tanker.ac</path>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>25000</max-m>
</animation>
<animation> 
    <type>textranslate</type> 
    <object-name>F-light</object-name> 
	<object-name>Accommodation</object-name>
	<object-name>Bridge</object-name>
	<object-name>DecHouse</object-name>
	<object-name>logo</object-name>
	 <object-name>superstrictures</object-name>
	<property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
    <center>
     <x-m> 0 </x-m>
     <y-m> 0 </y-m>
     <z-m> 0 </z-m>
    </center>
    <axis> 
      <x> -1 </x> 
      <y>  0 </y> 
      <z>  0 </z> 
    </axis> 
  </animation>
 
  <animation>
    <type>material</type>
    <object-name>F-light</object-name> 
	<object-name>Accommodation</object-name>
	<object-name>Bridge</object-name>
	<object-name>DecHouse</object-name>
	<object-name>logo</object-name>
	<object-name>superstrictures</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>F-light</object-name> 
	<object-name>Accommodation</object-name>
	<object-name>Bridge</object-name>
	<object-name>DecHouse</object-name>
	<object-name>logo</object-name>
	<object-name>superstrictures</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>
  
  <path>Tanker.ac</path>
<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>40000</max-m>
</animation>
	<!-- Luce di Prua Bianca -->
        <bec1Xparams>
		<light-near>0.4</light-near>
		<light-med>0.8</light-med>
		<light-far>10</light-far>
	</bec1Xparams>

	<animation>
		<name>bec1XGroup</name>
		<object-name>bec1X0</object-name>
		<object-name>bec1X1</object-name>
		<object-name>bec1X2</object-name>
	</animation>

	<animation>
		<type>select</type>
		<name>bec1XGroupNight</name>
		<object-name>bec1XGroup</object-name>
		<condition>
			<greater-than>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.57</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>alpha-test</type>
		<object-name>bec1XGroup</object-name>
		<alpha-factor>0.001</alpha-factor>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>bec1X0</object-name>
		<offset-m>114.901477</offset-m>
		<axis>
			<x>-112.75</x>
			<y>0.433013</y>
			<z>22.126892</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>bec1X1</object-name>
		<offset-m>114.901477</offset-m>
		<axis>
			<x>-112.75</x>
			<y>-0.433013</y>
			<z>22.126892</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>bec1X2</object-name>
		<offset-m>115.636713</offset-m>
		<axis>
			<x>-113.5</x>
			<y>0</y>
			<z>22.126892</z>
		</axis>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>bec1X0</object-name>
		<spherical type="bool">true</spherical>
	</animation>

	<animation>
		<type>billboard</type>
		<object-name>bec1X1</object-name>
